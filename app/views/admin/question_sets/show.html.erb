<div>
  <h1> <%= @question_set.title %> </h1>
  <div> <p>状態:<%= QuestionSet::STATUS[@question_set.status] %> </p></div>
  <div>
  <table>
    <tr>
      <th>No</th>
      <th>内容</th>
    </tr>
    <% (1..10).each do |i| %>
    <tr>
      <td> <%= i %> </td>
      <td> <%= question_content(@question_set.id, i) %> </td>
    </tr>
  <% end %>
  </table>
  </div>
  <div>
    <%= link_to "一覧へもどる", admin_question_sets_path %> 
  <%= link_to "編集", edit_admin_question_set_path(@question_set) %>
  <%= link_to "削除", :admin_question_set, method: :delete, data: {confirm: "削除しますか？"} %>
  </div>
</div>
